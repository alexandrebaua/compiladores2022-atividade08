algoritmo "teste"
variáveis
   i, x, y: inteiro;
   z: real;
   nome: caracter;
   V1: vetor[1..10] de inteiro;
   V2: vetor[1..10, 2..20] de real;
   V3: vetor[1..10, 2..20, 3..30] de caracter;

// principal
inicio
   i <- 0;
   repita
      escreva(".");
      i <- i + 1;
   até que i > 10;
   escreva("Ok");

fim.
